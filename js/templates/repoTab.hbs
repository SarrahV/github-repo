 <div class="repo-tab">
   <div class="repo-tab-left">
    <h3>{{name}}</h3>
    <h4>{{description}}</h4>
    <h5>{{updated_at}}</h5>
  </div>
  <div class="repo-tab-right">
  <ul class="repo-icons">
    <li>{{language}}</li>
    <li><span class="octicon octicon-star"></span>{{stargazers_count}}</li>
    <li><span class="octicon octicon-git-branch"></span>{{forks_count}}</li>
  </ul>
  </div>
</div>