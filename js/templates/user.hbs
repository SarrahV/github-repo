<div class="user-name">  
  <span><img src="{{avatar_url}}" alt=""></span>
  <h1>{{name}}</h1>
  <h3>{{login}}</h3>
</div>
<div class="user-details">
  <ul>
    <li><span class="octicon octicon-location"></span>{{location}}</li>
    <li><span class="octicon octicon-clock"></span> Joined on {{formatedDate}}</li>
  </ul>
</div>
<div class="stats">
  <ul>
    <li><span>{{followers}}</span><p>Followers</p></li>
    <li>
      <span>{{myStars}}</span>
      <p>Starred</p>
    </li>
    <li>
      <span>{{following}}</span>
      <p>Following</p>
    </li>
  </ul>
</div>



